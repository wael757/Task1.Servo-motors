// C++ code
//*Servoes mototr rotate from 0 degree to 90 degrees
#include <Servo.h>
Servo s1,s2,s3,s4,s5;




void setup(){
  //*Attaches the Servoes to the pins.
  s1.attach(8);
  s2.attach(9);
  s3.attach(10);
  s4.attach(11);
  s5.attach(12);

 
}

void loop () {
  //make the servoes rotate 90 degrees
  s1.write(90);
  s2.write(90);
  s3.write(90);
  s4.write(90);
  s5.write(90);

	delay(1000);
  //make the servoes rotate to origin position
  s1.write(0);
  s2.write(0);
  s3.write(0);
  s4.write(0);
  s5.write(0);

  	delay(1000);
 

}
